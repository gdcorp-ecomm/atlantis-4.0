<?xml version="1.0" encoding="utf-8" ?>
<validatefield type="string">
  <length min="9" max="255" failurecode="2">
    <description>
      <![CDATA[passwordMinimum]]>
    </description>
  </length>
  <regex matchisvalid="true" ignorecase="false" failurecode="10">
    <description>
      <![CDATA[passwordUcNumSpace]]>
    </description>
    <server><![CDATA[(?=^\S).*(?=.*[A-Z])(?=.*\d).*(?=\S$)]]></server>
    <client><![CDATA[(?=^\S).*(?=.*[A-Z])(?=.*\d).*(?=\S$)]]></client>
  </regex>
</validatefield>
