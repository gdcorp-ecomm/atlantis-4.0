<?xml version="1.0" encoding="utf-8" ?>
<validatefield type="string">
  <length min="9" failurecode="2">
    <description>
      <![CDATA[{0} must be minimum of 9 characters.]]>
    </description>
  </length>
  <regex matchisvalid="true" ignorecase="false" failurecode="10">
    <description>
      <![CDATA[{0} must contain at least 1 uppercase letter, 1 number, and cannot begin or end with a space.]]>
    </description>
    <server><![CDATA[(?=^\S).*(?=.*[A-Z])(?=.*\d).*(?=\S$)]]></server>
    <client><![CDATA[(?=^\S).*(?=.*[A-Z])(?=.*\d).*(?=\S$)]]></client>
  </regex>
</validatefield>
